<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record 
        id="view_tree_tag" 
        model="ir.ui.view"
    >
        <field name="name">Tag List</field>
        <field name="model">estate_property.tag</field>
        <field name="arch" type = "xml">
            <tree editable="bottom">
                <field name="name" />
                <field 
                    name="estate_id" 
                    widget="many2many_tags" 
                />
            </tree>
        </field>
    </record>

</odoo>
