<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_tree_types" model="ir.ui.view">
        <field name="name">Type List</field>
        <field name="model">estate.type</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="property_id"/>
            </tree>
        </field>
    </record>
    <record id="view_form_types" model="ir.ui.view">
    <field name="name">Type Form</field>
    <field name="model">estate.type</field>
    <field name="arch" type="xml">
    <form string="Types">
        <sheet>
            <field name="name"/>
            <field name="property_id"/>
        </sheet>
    </form>
    </field> 
    </record>
</odoo>
