<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="report_estate_property" model="ir.actions.report">
        <field name="name">Property offers</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_property_offers</field>
        <field name="report_file">estate.report_property_offers</field>
        <field name="print_report_name">'Property offers report'</field>
        <!-- <field name="paperformat_id" ref="event.paperformat_euro_lowmargin" /> -->
        <field name="binding_model_id" ref="model_estate_property" />
        <field name="binding_type">report</field>
    </record>
    <record id="report_user_estate_property" model="ir.actions.report">
        <field name="name">User Properties offers</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_property_offers_user</field>
        <field name="report_file">estate.report_property_offers_user</field>
        <field name="print_report_name">'Properties and Offers for User'</field>
        <!-- <field name="paperformat_id" ref="event.paperformat_euro_lowmargin" /> -->
        <field name="binding_model_id" ref="base.model_res_users" />
        <field name="binding_type">report</field>
    </record>
</odoo>